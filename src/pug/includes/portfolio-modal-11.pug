#portfolioModal11.portfolio-modal.modal.fade(tabindex='-1', role='dialog', aria-hidden='true')
    .modal-dialog
        .modal-content
            .close-modal(data-bs-dismiss='modal')
                img(src='assets/img/close-icon.svg', alt='Close modal')
            .container
                .row.justify-content-center
                    .col-lg-8
                        .modal-body
                            // Project details
                            h2.text-uppercase Diane Hiebert
                            p.item-intro.text-muted [Say something special]
                            img.img-fluid.d-block.mx-auto(src='assets/img/portfolio/diane.jpg', alt='Diane Hiebert')
                            ul.list-inline
                                li
                                    strong Name:
                                    | 'Dizzy' Diane Hiebert
                                li
                                    strong Age:
                                    | 0
                                li
                                    strong Hometown:
                                    | Abbotsford, BC
                                li
                                    strong Stance:
                                    | Regular
                                li
                                    strong Sponsors:
                                    | [RIDER SPONSORS]
                            p Rider Bio TBA.
                            a.btn.btn-dark.btn-social.mx-2(href='https://www.instagram.com/di_zzy_j/', aria-label='Diane Hiebert Instagram Profile')
                                i.fab.fa-instagram
                            a.btn.btn-dark.btn-social.mx-2(href='https://www.youtube.com/watch?v=P6uHN5q_lJ4', aria-label='Diane Hiebert Youtube')
                                i.fab.fa-youtube
                            //a.btn.btn-dark.btn-social.mx-2(href='#!', aria-label='Diane Hiebert LinkedIn Profile')
                            //    i.fab.fa-linkedin-in
                            p
                            button.btn.btn-primary.btn-xl.text-uppercase(data-bs-dismiss='modal', type='button')
                                i.fas.fa-xmark.me-1
                                | Close
